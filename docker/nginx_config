server {
    listen 80;
    server_name teahaz.co.uk;

    location ~ /.well-known {
        root /home/teahaz/static;
    }

    location / {
        include proxy_params;
        proxy_pass http://localhost:13337;
    }
}


server {
    listen 443;
    server_name teahaz.co.uk;

    location ~ /.well-known {
        root /home/teahaz/static;
    }

    location / {
        include proxy_params;
        proxy_pass http://localhost:13337;
    }
}
